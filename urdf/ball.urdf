<robot name="ball">
    <link name="base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius=".2"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius=".2"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1"/>
            <inertia ixx="8e-03" ixy="-4-05" ixz="1e-04"
                                 iyy="8e-03" iyz="-4-06"
                                             izz="8e-03" />
        </inertial>
    </link>
    <gazebo reference="base_link">
        <mu1>1</mu1>
        <mu2>1</mu2>
        <kp>500000</kp>
        <kd>0</kd>
        <minDepth>0</minDepth>
        <maxVel>1000</maxVel>
	<material>Gazebo/Yellow</material>
    </gazebo>
</robot>
